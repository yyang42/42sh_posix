compound_list
    ast_list_item (sep=\n)
        ast_andor_item (negate=false, sep=none)
            ast_pipe_item
                ast_command
                    ast_compound_command
                        ast_until_clause
                            cond_compound
                                compound_list
                                    ast_list_item (sep=;)
                                        ast_andor_item (negate=false, sep=none)
                                            ast_pipe_item
                                                ast_command
                                                    ast_simple_command (false)
                            do_group
                                compound_list
                                    ast_list_item (sep=\n)
                                        ast_andor_item (negate=false, sep=none)
                                            ast_pipe_item
                                                ast_command
                                                    ast_simple_command (echo abc)
